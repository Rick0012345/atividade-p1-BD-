# Exemplo de arquivo .env para configuração do MongoDB
# Copie este arquivo para .env e configure suas credenciais

# MongoDB Atlas (Nuvem)
MONGODB_ATLAS_URI=mongodb+srv://username:password@cluster.mongodb.net/
MONGODB_DATABASE=crud_database

# Configurações opcionais
MONGODB_USERNAME=admin
MONGODB_PASSWORD=password
MONGODB_HOST=localhost
MONGODB_PORT=27017

# Ambiente de desenvolvimento
ENVIRONMENT=development
DEBUG=True